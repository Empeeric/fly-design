{#menu}
    {#items}
        <a {?dock}class="active{:else}href="{url}"{/dock}">{title}</a>

        {@if cond="'{template}' == 'projects' "}
        <nav class="second {^dock}hide{/dock}">
            <div class="container">
                {#sub.items}
                    <a href="{url}" {?dock}class="active"{/dock}>{title}</a>
                {/sub.items}
            </div>
        </nav>
        {/if}
    {/items}
{/menu}
